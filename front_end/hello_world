"use strict";
//declare message variable
let message;
let roomsizex;
let roomsizey;
let initialx;
let initialy;
let positionx;
let positiony;
let speedx;
let speedy;
let color;

roomsizex = 200;
roomsizey = 300;
initialx = 100;
initialy = 100;
speedx = 10;
speedy = 10;
positionx = speedx + initialx;
positiony = speedy + initialy;
color = "orange";


//defining message variable
message = "Q: What is another word for a change in speed?";
//sending the message by alert
[roomsizex, roomsizey, initialx, initialy, positionx, positiony, speedx, speedy, color].forEach(alert);